    "build:styles": "postcss src/**/*.css src/*.css --base src -d dist/"
  },
  "dependencies": {
    "vue": "3.5.12"
  },
  "devDependencies": {
    "@repo/typescript-config": "*",
    "@types/node": "22.8.4",
    "@vitejs/plugin-vue": "5.1.4",
    "autoprefixer": "10.4.20",
    "cssnano": "7.0.6",
    "glob": "11.0.0",
    "postcss": "8.4.47",
    "postcss-cli": "11.0.0",
    "postcss-import": "16.1.0",
    "rollup": "4.24.3",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vue-tsc": "2.1.8"
  }
}
